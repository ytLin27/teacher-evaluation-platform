{
  "dashboard": {
    "id": null,
    "title": "Teacher Evaluation Platform - System Overview",
    "tags": ["teacher-evaluation", "overview", "microservices"],
    "style": "dark",
    "timezone": "browser",
    "editable": true,
    "hideControls": false,
    "graphTooltip": 1,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "refresh": "30s",
    "version": 1,
    "panels": [
      {
        "id": 1,
        "title": "System Health Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "up",
            "legendFormat": "{{job}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "green", "value": 1}
              ]
            },
            "mappings": [
              {"type": "value", "value": "0", "text": "DOWN"},
              {"type": "value", "value": "1", "text": "UP"}
            ]
          }
        },
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 0},
        "options": {
          "reduceOptions": {
            "calcs": ["lastNotNull"]
          },
          "orientation": "horizontal",
          "textMode": "auto",
          "colorMode": "background"
        }
      },
      {
        "id": 2,
        "title": "Request Rate (RPS)",
        "type": "graph",
        "targets": [
          {
            "expr": "sum by (job) (rate(http_requests_total[5m]))",
            "legendFormat": "{{job}}",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 9, "w": 12, "x": 0, "y": 8},
        "yAxes": [
          {
            "label": "Requests/sec",
            "min": 0
          },
          {
            "show": false
          }
        ],
        "legend": {
          "show": true,
          "values": true,
          "current": true,
          "avg": true
        },
        "tooltip": {
          "shared": true,
          "sort": 2
        }
      },
      {
        "id": 3,
        "title": "Response Time (95th percentile)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le, job))",
            "legendFormat": "{{job}}",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 9, "w": 12, "x": 12, "y": 8},
        "yAxes": [
          {
            "label": "Seconds",
            "min": 0
          },
          {
            "show": false
          }
        ],
        "legend": {
          "show": true,
          "values": true,
          "current": true,
          "avg": true
        },
        "tooltip": {
          "shared": true,
          "sort": 2
        }
      },
      {
        "id": 4,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "sum by (job) (rate(http_requests_total{status=~\"5..\"}[5m])) / sum by (job) (rate(http_requests_total[5m]))",
            "legendFormat": "{{job}}",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 9, "w": 12, "x": 0, "y": 17},
        "yAxes": [
          {
            "label": "Error Rate",
            "min": 0,
            "max": 1,
            "unit": "percentunit"
          },
          {
            "show": false
          }
        ],
        "legend": {
          "show": true,
          "values": true,
          "current": true,
          "avg": true
        },
        "tooltip": {
          "shared": true,
          "sort": 2
        },
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [0.05], "type": "gt"},
              "operator": {"type": "and"},
              "query": {"params": ["A", "5m", "now"]},
              "reducer": {"type": "avg"},
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "for": "2m",
          "frequency": "10s",
          "handler": 1,
          "name": "High Error Rate Alert",
          "noDataState": "no_data"
        }
      },
      {
        "id": 5,
        "title": "CPU Usage by Service",
        "type": "graph",
        "targets": [
          {
            "expr": "sum by (service_name) (rate(container_cpu_usage_seconds_total[5m])) * 100",
            "legendFormat": "{{service_name}}",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 9, "w": 12, "x": 12, "y": 17},
        "yAxes": [
          {
            "label": "CPU %",
            "min": 0,
            "max": 100
          },
          {
            "show": false
          }
        ],
        "legend": {
          "show": true,
          "values": true,
          "current": true,
          "avg": true
        },
        "tooltip": {
          "shared": true,
          "sort": 2
        }
      },
      {
        "id": 6,
        "title": "Memory Usage by Service",
        "type": "graph",
        "targets": [
          {
            "expr": "sum by (service_name) (container_memory_usage_bytes) / 1024 / 1024",
            "legendFormat": "{{service_name}}",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 9, "w": 12, "x": 0, "y": 26},
        "yAxes": [
          {
            "label": "Memory (MB)",
            "min": 0
          },
          {
            "show": false
          }
        ],
        "legend": {
          "show": true,
          "values": true,
          "current": true,
          "avg": true
        },
        "tooltip": {
          "shared": true,
          "sort": 2
        }
      },
      {
        "id": 7,
        "title": "Database Connections",
        "type": "graph",
        "targets": [
          {
            "expr": "pg_stat_database_numbackends",
            "legendFormat": "PostgreSQL - {{datname}}",
            "refId": "A"
          },
          {
            "expr": "mongodb_connections{state=\"current\"}",
            "legendFormat": "MongoDB - Current",
            "refId": "B"
          },
          {
            "expr": "redis_connected_clients",
            "legendFormat": "Redis - Clients",
            "refId": "C"
          }
        ],
        "gridPos": {"h": 9, "w": 12, "x": 12, "y": 26},
        "yAxes": [
          {
            "label": "Connections",
            "min": 0
          },
          {
            "show": false
          }
        ],
        "legend": {
          "show": true,
          "values": true,
          "current": true,
          "avg": true
        },
        "tooltip": {
          "shared": true,
          "sort": 2
        }
      },
      {
        "id": 8,
        "title": "Business Metrics - Evaluations",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(evaluation_submissions_total)",
            "refId": "A"
          },
          {
            "expr": "sum(rate(evaluation_submissions_total[1h]))",
            "refId": "B"
          },
          {
            "expr": "sum(evaluation_submissions_total{status=\"failed\"}) / sum(evaluation_submissions_total)",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Failure Rate"},
              "properties": [
                {"id": "unit", "value": "percentunit"},
                {"id": "max", "value": 1},
                {"id": "min", "value": 0}
              ]
            }
          ]
        },
        "gridPos": {"h": 6, "w": 8, "x": 0, "y": 35},
        "options": {
          "reduceOptions": {
            "calcs": ["lastNotNull"]
          },
          "orientation": "horizontal",
          "textMode": "auto",
          "colorMode": "value"
        },
        "transformations": [
          {
            "id": "organize",
            "options": {
              "renameByName": {
                "Value #A": "Total Evaluations",
                "Value #B": "Evaluations/Hour",
                "Value #C": "Failure Rate"
              }
            }
          }
        ]
      },
      {
        "id": 9,
        "title": "Authentication Metrics",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(auth_attempts_total)",
            "refId": "A"
          },
          {
            "expr": "sum(auth_attempts_total{status=\"failed\"}) / sum(auth_attempts_total)",
            "refId": "B"
          },
          {
            "expr": "keycloak_user_sessions",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Failure Rate"},
              "properties": [
                {"id": "unit", "value": "percentunit"},
                {"id": "max", "value": 1},
                {"id": "min", "value": 0}
              ]
            }
          ]
        },
        "gridPos": {"h": 6, "w": 8, "x": 8, "y": 35},
        "options": {
          "reduceOptions": {
            "calcs": ["lastNotNull"]
          },
          "orientation": "horizontal",
          "textMode": "auto",
          "colorMode": "value"
        },
        "transformations": [
          {
            "id": "organize",
            "options": {
              "renameByName": {
                "Value #A": "Total Auth Attempts",
                "Value #B": "Auth Failure Rate",
                "Value #C": "Active Sessions"
              }
            }
          }
        ]
      },
      {
        "id": 10,
        "title": "Data Quality Metrics",
        "type": "stat",
        "targets": [
          {
            "expr": "analytics_data_quality_score",
            "refId": "A"
          },
          {
            "expr": "analytics_evaluation_anomaly_rate",
            "refId": "B"
          },
          {
            "expr": "analytics_prediction_accuracy",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 0.7},
                {"color": "green", "value": 0.85}
              ]
            },
            "unit": "percentunit",
            "max": 1,
            "min": 0
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Anomaly Rate"},
              "properties": [
                {"id": "unit", "value": "percent"},
                {"id": "max", "value": 100},
                {"id": "thresholds", "value": {
                  "steps": [
                    {"color": "green", "value": 0},
                    {"color": "yellow", "value": 10},
                    {"color": "red", "value": 20}
                  ]
                }}
              ]
            }
          ]
        },
        "gridPos": {"h": 6, "w": 8, "x": 16, "y": 35},
        "options": {
          "reduceOptions": {
            "calcs": ["lastNotNull"]
          },
          "orientation": "horizontal",
          "textMode": "auto",
          "colorMode": "background"
        },
        "transformations": [
          {
            "id": "organize",
            "options": {
              "renameByName": {
                "Value #A": "Data Quality Score",
                "Value #B": "Anomaly Rate",
                "Value #C": "Prediction Accuracy"
              }
            }
          }
        ]
      },
      {
        "id": 11,
        "title": "Top 10 Slowest Endpoints",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[10m])) by (le, job, method, handler)))",
            "format": "table",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 41},
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "auto",
              "displayMode": "auto"
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Value"},
              "properties": [
                {"id": "displayName", "value": "Response Time (95th)"},
                {"id": "unit", "value": "s"},
                {"id": "custom.displayMode", "value": "lcd-gauge"}
              ]
            }
          ]
        },
        "options": {
          "showHeader": true
        },
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "__name__": true,
                "Time": true,
                "le": true
              },
              "renameByName": {
                "job": "Service",
                "method": "Method",
                "handler": "Endpoint"
              }
            }
          }
        ]
      },
      {
        "id": 12,
        "title": "Alert Status",
        "type": "table",
        "targets": [
          {
            "expr": "ALERTS{alertstate=\"firing\"}",
            "format": "table",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 41},
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "auto",
              "displayMode": "auto"
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "severity"},
              "properties": [
                {"id": "custom.displayMode", "value": "color-background"},
                {"id": "mappings", "value": [
                  {"type": "value", "value": "critical", "text": "CRITICAL", "color": "red"},
                  {"type": "value", "value": "warning", "text": "WARNING", "color": "orange"},
                  {"type": "value", "value": "info", "text": "INFO", "color": "blue"}
                ]}
              ]
            }
          ]
        },
        "options": {
          "showHeader": true
        },
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "__name__": true,
                "Time": true,
                "alertstate": true,
                "instance": true
              },
              "renameByName": {
                "alertname": "Alert",
                "job": "Service",
                "severity": "Severity"
              }
            }
          }
        ]
      }
    ],
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "Prometheus",
          "enable": true,
          "expr": "deployment_timestamp",
          "iconColor": "rgba(0, 211, 255, 1)",
          "limit": 100,
          "showIn": 0,
          "step": "60s",
          "titleFormat": "Deployment",
          "type": "tags"
        }
      ]
    },
    "templating": {
      "list": [
        {
          "name": "service",
          "type": "query",
          "query": "label_values(up, job)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*",
          "multi": true,
          "regex": "",
          "datasource": "Prometheus"
        },
        {
          "name": "interval",
          "type": "interval",
          "query": "30s,1m,5m,10m,30m,1h",
          "refresh": 2,
          "current": {
            "text": "5m",
            "value": "5m"
          },
          "auto": true,
          "auto_count": 30,
          "auto_min": "10s"
        }
      ]
    },
    "links": [
      {
        "asDropdown": false,
        "icon": "external link",
        "includeVars": false,
        "keepTime": false,
        "tags": ["teacher-evaluation"],
        "targetBlank": true,
        "title": "Related Dashboards",
        "type": "dashboards"
      }
    ]
  },
